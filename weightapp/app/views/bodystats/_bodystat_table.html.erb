<div class="col-xs-12">
		<div class="panel panel-default">
				<table id="table" class="table table-striped table-responsive">
	 				<thead class="thead-inverse">
						<tr>
							<th>ID</th>
							<th>Date</th>
							<th>Body Weight lbs</th>
							<th>Body Water %</th>
							<th>Body Fat %</th>
							<th>Visceral Fat ?</th>
							<th>Muscle Mass KG</th>
							<th>BMI</th>
						</tr>
					</thead>
					<tbody>
						<% @bodystats.each do |stat| %>
							<tr>
								<td><%= stat.id %></td>
								<td><%= time_ago_in_words(stat.date)  %></td>
								<td><%= stat.body_weight * 2.205 %></td>
								<td><%= stat.body_water %></td>
								<td><%= stat.body_fat %></td>
								<td><%= stat.visceral_fat %></td>
								<td><%= stat.muscle_mass %></td>
								<td><%= stat.bmi %></td>
							</tr>
						<% end %>
					</tbody>
				</table>

		</div>
	<div>
</div>